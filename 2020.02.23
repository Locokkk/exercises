//字符串最后一个单词的长度


#include<iostream>
#include<string>
using namespace std;
 
int main()
{
    string s;
    //cin>>s;
    getline(cin,s);
    size_t pos=s.rfind(" ");
    if(pos!=string::npos)
    {
        cout<<s.size()-pos-1<<endl;
    }
    else{
        cout<<s.size()<<endl;
    }
    return 0;
}
